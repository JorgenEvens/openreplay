{
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    // "module": "es6",
    // "target": "es5",
    "allowJs": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noEmit": true,
    "jsx": "react",
    // "baseUrl": ".",
  },
  "include": ["app"]
}